{
  "custom/brightness-py": {
    "interval": 1,
    "format": "{icon} {percentage}%",
    "format-icons": [
      "󰽧",
      "",
      "󰖨"
  ],
    "exec": "python ~/dotfiles/waybar/modules/brightness.py",
    "exec-on-event": false,
    "return-type": "json"
  },
  "pulseaudio": {
    "format": "{icon} {volume}%",
    "format-icons": [
      "󰕿",
      "󰖀",
      "󰕾"
    ],
    "on-click-right": "pavucontrol"
  },
  "group/sources": {
    "orientation": "horizontal",
    "modules": [
      "custom/brightness-py",
      "pulseaudio"
    ]
  }
}
